{
  "name": "SubjectTag",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Easy input tags in subject's email.",
  "icons" : {
    "48" : "icon_48.png",
    "128" : "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "SubjectTag"
  },
  "options_page": "options.html",
  "permissions": [
    "https://mail.google.com/",
    "tabs",
    "*://mail.google.com/*"
   ]
}